            <ImageView fitHeight="57.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../User-Interface-Logout-icon.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="addContacts" disable="true" layoutX="175.0" layoutY="407.0" mnemonicParsing="false" onAction="#addContact" prefHeight="36.0" prefWidth="165.0" style="-fx-background-color: transparent; -fx-background-radius: 10;" text="add to contacts" textFill="#101820">
