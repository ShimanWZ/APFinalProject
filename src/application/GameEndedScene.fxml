<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onMouseMoved="#initializeScene" prefHeight="350.0" prefWidth="500.0" style="-fx-background-color: #f2AA4cff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GameEndedController">
   <children>
      <ImageView fitHeight="312.0" fitWidth="471.0" layoutX="14.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../border.png" />
         </image>
      </ImageView>
      <Label fx:id="playerWins" layoutX="89.0" layoutY="220.0" textFill="#101820">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font></Label>
      <Label fx:id="playerLosses" layoutX="89.0" layoutY="256.0" textFill="#101820">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font></Label>
      <Label fx:id="comLosses" layoutX="89.0" layoutY="184.0" textFill="#101820">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font></Label>
      <Label fx:id="gameStatus" layoutX="98.0" layoutY="71.0" text="The game was ended..." textFill="#101820">
         <font>
            <Font name="Arial Rounded MT Bold" size="27.0" />
         </font></Label>
      <Label fx:id="compWins" layoutX="89.0" layoutY="150.0" textFill="#101820">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font></Label>
      <Button layoutX="436.0" layoutY="14.0" mnemonicParsing="false" onAction="#closeWindow" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="64.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../cancel-icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="14.0" layoutY="11.0" mnemonicParsing="false" onAction="#startANewGame" prefHeight="32.0" prefWidth="143.0" style="-fx-background-color: transparent; -fx-border-color: black; -fx-border-radius: 10; -fx-border-width: 2;" text="start new game...">
         <font>
            <Font name="Arial Rounded MT Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
