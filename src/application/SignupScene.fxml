      <PasswordField fx:id="passConfirm" layoutX="50.0" layoutY="313.0" prefHeight="27.0" prefWidth="250.0" promptText="confirm password" style="-fx-background-color: transparent; -fx-border-color: #101820ff; -fx-border-radius: 30; -fx-prompt-text-fill: #101820ff;" />
