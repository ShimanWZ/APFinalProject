<AnchorPane maxHeight="537.0" maxWidth="350.0" minHeight="537.0" minWidth="350.0" onMouseMoved="#loadChoiceBoxItems" prefHeight="537.0" prefWidth="350.0" style="-fx-background-color: #f2AA4cff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SignupHandler">
