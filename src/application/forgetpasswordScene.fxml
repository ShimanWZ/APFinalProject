<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="500.0" style="-fx-background-color: #f2AA4cff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ForgetPasswordController">
   <children>
      <Label fx:id="questionField" layoutX="54.0" layoutY="175.0" textFill="#2f718a">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="answerField" disable="true" layoutX="99.0" layoutY="210.0" prefHeight="26.0" prefWidth="302.0" style="-fx-background-color: transparent; -fx-border-color: #101820ff; -fx-border-radius: 5;" />
      <Button layoutX="378.0" layoutY="253.0" mnemonicParsing="false" onAction="#passwordAndQuestionFieldController" prefHeight="26.0" prefWidth="99.0" style="-fx-background-color: #101820ff; -fx-border-radius: 15; -fx-background-radius: 15;" text="submit" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="15.0" />
         </font>
      </Button>
      <Label fx:id="passwordField" layoutX="54.0" layoutY="296.0" prefHeight="18.0" prefWidth="391.0" textFill="#2f718a">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="54.0" layoutY="70.0" text="enter our username:" textFill="#101820">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="98.0" layoutY="106.0" prefHeight="26.0" prefWidth="302.0" style="-fx-background-color: transparent; -fx-border-color: #101820ff; -fx-border-radius: 5;" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #101820ff; -fx-background-radius: 5;" text="back" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
